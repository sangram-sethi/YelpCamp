{
  "version": 2,
  "builds": [
    {
      "src": "yelpcamp/app.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "yelpcamp/app.js"
    },
    {
      "handle": "filesystem"
    }
  ],
  "static": {
    "public": "yelpcamp/public"
  },
  "functions": {
    "yelpcamp/app.js": {
      "memory": 512,
      "maxDuration": 10
    },
    "api/index.js": {
      "memory": 512,
      "maxDuration": 10
    }
  }
}