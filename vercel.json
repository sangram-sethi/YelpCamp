{
  "version": 2,
  "builds": [
    {
      "src": "/app.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/app.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "public": true
    }
  ]
}